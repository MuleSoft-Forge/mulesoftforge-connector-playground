{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TaskIdParams",
  "description": "Parameters for operations that require only a task ID in A2A protocol",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier of the task"
    },
    "metadata": {
      "type": "object",
      "description": "Additional metadata for the operation",
      "additionalProperties": true
    }
  },
  "required": ["id"]
} 